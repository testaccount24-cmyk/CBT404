//SBGOLOBT JOB ,'SAM GOLOB',CLASS=B,
//    MSGCLASS=X,NOTIFY=&SYSUID
//*
//*      PDSDOC *ASSEMBLE TSSO (ALL MODULES)..........................*
//*
//ASMTSSO PROC MEMBER=,RENT=NORENT
//ASM    EXEC  PGM=ASMA90,REGION=6M,
//         PARM=('&RENT,OBJECT,NODECK,SIZE(MAX,ABOVE),BATCH')
//SYSUT1   DD DSN=&&SYSUT1,UNIT=VIO,SPACE=(CYL,(20,20))
//SYSUT2   DD DSN=&&SYSUT2,UNIT=VIO,SPACE=(CYL,(20,20))
//SYSUT3   DD DSN=&&SYSUT3,UNIT=VIO,SPACE=(CYL,(20,20))
//SYSPUNCH DD SYSOUT=B
//SYSLIN   DD DSN=&&OBJECT,DISP=(,PASS,DELETE),
//         UNIT=VIO,SPACE=(CYL,(1,1))
//SYSLIB   DD DISP=SHR,DSN=SBGOLOB.TSSOZ18.INSTALL
//         DD DISP=SHR,DSN=SBGOLOB.R18.MACLIB
//         DD DISP=SHR,DSN=SBGOLOB.R18.MODGEN
//SYSPRINT DD SYSOUT=*
//SYSIN    DD DSN=SBGOLOB.CBT479.FILE404(&MEMBER),DISP=SHR
//SYSTERM  DD SYSOUT=*
//LKED     EXEC PGM=IEWL,REGION=6M,COND=(4,LT,ASM),
//         PARM=(XREF,LIST,MAP,LET,NCAL,&RENT)
//SYSPRINT DD SYSOUT=*
//SYSUT1   DD DSN=&&SYSUT1,UNIT=SYSALLDA,SPACE=(CYL,(20,20))
//SYSLIN   DD DSN=&&OBJECT,DISP=(OLD,DELETE)
//         DD DDNAME=SYSIN
//SYSLMOD  DD DSN=SBGOLOB.TSSOZ18.LOAD,DISP=SHR
//SYSIN DD DUMMY
// PEND
//*********************************************************************
//* THE FIRST TWO MODULES ARE SUBROUTINES REQUIRED BY THE REST OF THE *
//* MODULES.                                                          *
//*********************************************************************
//*CECVT   EXEC ASMTSSO,MEMBER=JCECVT,RENT=RENT
//*SSOPARS EXEC ASMTSSO,MEMBER=TSSOPARS,RENT=RENT
//*********************************************************************
//* THE NEXT GROUP OF MODULES HANDLE TSSO INITILIZATION.              *
//*********************************************************************
//*SSO     EXEC ASMTSSO,MEMBER=TSSO,RENT=RENT
//*SSOINIT EXEC ASMTSSO,MEMBER=TSSOINIT,RENT=RENT
//*SSOINI1 EXEC ASMTSSO,MEMBER=TSSOINI1,RENT=RENT
//*SSOINI2 EXEC ASMTSSO,MEMBER=TSSOINI2,RENT=RENT
//*SSOINI3 EXEC ASMTSSO,MEMBER=TSSOINI3,RENT=RENT
//*SSOINI4 EXEC ASMTSSO,MEMBER=TSSOINI4,RENT=RENT
//*SSOINI5 EXEC ASMTSSO,MEMBER=TSSOINI5,RENT=RENT
//*SSOGSSC EXEC ASMTSSO,MEMBER=TSSOGSSC,RENT=RENT
//*
//*********************************************************************
//* THE NEXT GROUP OF MODULES ARE TSSO MAINLINE CODE AND SUBS         *
//*********************************************************************
//*SSOMAIN EXEC ASMTSSO,MEMBER=TSSOMAIN,RENT=RENT
//*SSOWTO  EXEC ASMTSSO,MEMBER=TSSOWTO,RENT=RENT
//TSSOSS09 EXEC ASMTSSO,MEMBER=TSSOSS09,RENT=RENT
//*SSOSS10 EXEC ASMTSSO,MEMBER=TSSOSS10,RENT=RENT
//*SSOTERM EXEC ASMTSSO,MEMBER=TSSOTERM,RENT=RENT
//*SSOESTA EXEC ASMTSSO,MEMBER=TSSOESTA,RENT=RENT
//*SSODOCM EXEC ASMTSSO,MEMBER=TSSODOCM,RENT=RENT
//*SSOCCMD EXEC ASMTSSO,MEMBER=TSSOCCMD,RENT=RENT
//*SSLRACF EXEC ASMTSSO,MEMBER=TSSLRACF,RENT=RENT
//*SSOLAOF EXEC ASMTSSO,MEMBER=TSSOLAOF,RENT=RENT
//*
//*********************************************************************
//* THE NEXT GROUP OF MODULES MAKE UP THE TSO AND NCCF VERSION OF     *
//* THE OSCMD FACILITY.                                               *
//*********************************************************************
//*SSOPREC EXEC ASMTSSO,MEMBER=TSSOPREC,RENT=RENT
//*SSOPCMD EXEC ASMTSSO,MEMBER=TSSOPCMD,RENT=RENT
//*SCMDATH EXEC ASMTSSO,MEMBER=OSCMDATH,RENT=RENT
//*PCMDMSG EXEC ASMTSSO,MEMBER=OPCMDMSG,RENT=RENT
//*SCMD    EXEC ASMTSSO,MEMBER=OSCMD,RENT=RENT
//*  //NCFOSATH EXEC ASMTSSO,MEMBER=NCFOSATH,RENT=RENT
//*  //NCFOSCMD EXEC ASMTSSO,MEMBER=NCFOSCMD,RENT=RENT
//*
//*********************************************************************
//* THE NEXT GROUP OF MODULES MAKE UP THE TSSO/OPF OSWAIT FACILITY.   *
//* THE OSCMD FACILITY.                                               *
//*********************************************************************
//*SWAITRC EXEC ASMTSSO,MEMBER=OSWAITRC,RENT=RENT
//*SWAIT   EXEC ASMTSSO,MEMBER=OSWAIT,RENT=RENT
//*********************************************************************
//* THE NEXT GROUP OF MODULES ARE THE TSSO/OPF OS FAMILY OF COMMANDS  *
//* THE OSCMD FACILITY.                                               *
//*********************************************************************
//*SASK    EXEC ASMTSSO,MEMBER=OSASK,RENT=RENT
//*SPAUSE  EXEC ASMTSSO,MEMBER=OSPAUSE,RENT=RENT
//*SWTO    EXEC ASMTSSO,MEMBER=OSWTO,RENT=RENT
//*
//*********************************************************************
//* THE NEXT MODULE IS THE TSSO CROSS MEMORY SERVICES ROUTINE.        *
//*********************************************************************
//*SSOGAPF EXEC ASMTSSO,MEMBER=TSSOGAPF,RENT=RENT
//*
//*********************************************************************
//* THE NEXT MODULES ARE THE COMMANDS TO CONTROL TSSO/AOF.            *
//*********************************************************************
//*ISPAOF  EXEC ASMTSSO,MEMBER=DISPAOF,RENT=NORENT
//*LTAOF   EXEC ASMTSSO,MEMBER=ALTAOF,RENT=NORENT
//*SSORATH EXEC ASMTSSO,MEMBER=TSSORATH,RENT=RENT
//*
//*********************************************************************
//* THE NEXT MODULES MAKE UP THE TSSO REPLY COMMAND PROCESSOR.        *
//*********************************************************************
//*EPLY    EXEC ASMTSSO,MEMBER=REPLY,RENT=NORENT
//*IONRPLY EXEC ASMTSSO,MEMBER=LIONRPLY,RENT=NORENT
//*EPLYLOA EXEC ASMTSSO,MEMBER=REPLYLOA,RENT=NORENT
//*
//*********************************************************************
//* THE NEXT MODULES ARE UTILITY COMMANDS THAT WORK WELL UNDER TSSO.  *
//*********************************************************************
//*ACHE    EXEC ASMTSSO,MEMBER=CACHE,RENT=NORENT
//*PCMD    EXEC ASMTSSO,MEMBER=CPCMD,RENT=NORENT
//*PMON    EXEC ASMTSSO,MEMBER=SPMON,RENT=NORENT
//*ALLOC   EXEC ASMTSSO,MEMBER=VALLOC,RENT=NORENT
//*OLSER   EXEC ASMTSSO,MEMBER=VOLSER,RENT=NORENT
//*
//*********************************************************************
//* THE NEXT MODULES WERE WRITTEN BY BILL GODFREY...                  *
//*********************************************************************
//*F       EXEC ASMTSSO,MEMBER=EF,RENT=RENT
//*ULT     EXEC ASMTSSO,MEMBER=MULT,RENT=RENT
